<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.PracticaM08UF1" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/blue_primary</item>
        <item name="colorPrimaryVariant">@color/blue_primary</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_no_focused</item>
        <item name="colorSecondaryVariant">@color/blue_no_focused</item>
        <item name="colorOnSecondary">@color/white</item>

    </style>

    <style name="Theme.PracticaM08UF1.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.PracticaM08UF1.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.PracticaM08UF1.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>